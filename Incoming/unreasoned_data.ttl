@prefix dwc:     <http://rs.tdwg.org/dwc/terms/> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ark:     <http://biscicol.org/id/ark:> .
@prefix obo:     <http://purl.obolibrary.org/obo/> .

# logical axiom to infer vascular leaf phenological stage:
#'plant phenological stage'
# and ('has participant' some 
#    ('whole plant'
#     and ('has quality' some 'leaves present')))

# We want to infer instances of "'whole plant' that 'participates in' some 'vascular leaf phenological stage'"

# leaves present:
#'leaf presence'
# and ('quality of' some 
#    ('whole plant'
#     and ('has part' some 'true leaf')))

#<http://purl.obolibrary.org/obo/RO_0000056> #'participates in'

# Need to declare predicates as objectProperties 
obo:RO_0000086 a owl:ObjectProperty .
obo:RO_0000057 a owl:ObjectProperty .
obo:BFO_0000051 a owl:ObjectProperty .

# plant1
# trait indicating leaves present 
<http://biscicol.org/test/?plant1>
      	a obo:PO_0000003;  				 # 'whole plant'
        obo:RO_0000086 <http://biscicol.org/test/?trait1>; # 'has quality' some 'leaves present'
	rdfs:label "plant1 should infer 4000 plant with leaves, 4050 vas. leaf expansion stage" .

# plant2
# traits indicating senescent leaves present and expanding leaves present 
<http://biscicol.org/test/?plant2>
      	a obo:PO_0000003;  				 # 'whole plant'
        obo:RO_0000086 <http://biscicol.org/test/?trait2>; # 'has quality' some 'senescent leaves present' 
        obo:RO_0000086 <http://biscicol.org/test/?trait3>; # 'has quality' some 'expanding leaves present'
	rdfs:label "plant2 should infer 4000 plant with leaves, 4001 plant with expanding leaves, 4003 plant with sen. leaves, 4050 vas. leaf expansion stage, 4051 plant in vasc. leaf expansion stage, 4053 plant in sen. leaf expansion stage" .

# plant3
# trait indicating leaf presence
# Putting aside, for now dealing with counts, e.g. lower count = 1 and upper count not specified
# for now just saying 'has part' some 'true leaf'
<http://biscicol.org/test/?plant3>
      	a obo:PO_0000003;  				 # 'whole plant'
        obo:RO_0000086 <http://biscicol.org/test/?trait4>; # 'has quality' some 'leaf presence'
        obo:BFO_0000051 <http://biscicol.org/test/?trait5>;  # 'has part' some 'true leaf' 
	rdfs:label "plant3 should infer 4000 plant with leaves, 4050 'vas. leaf expansion stage'" .

# plant4
# use of measurement datum and lower count to infer a plant with leaves/ vas. leaf expansion stage
# makes use of the following axiom (need to add to ontology by hand for now)
#'leaf presence' and (
#  'quality of' some (
#    'whole plant' and (
#      has_specified_output some (
#        'phenology observing process' and (
#          has_specified_input some ( 
#            'measurement datum' AND (
#              'lower count' min 1 xsd:integer 
#            )
#	   )  and 'is about' some 'leaf presence'
#	 )
#      ) 
#    )
#  )
#)
<http://biscicol.org/test/?plant4>
      	a obo:PO_0000003;  				 # 'whole plant'
        obo:RO_0000086 <http://biscicol.org/test/?trait6>; # 'has quality' some 'leaf presence'
	#obo:OBI_0000293 <http://biscicol.org/test/?observingprocess1> . # 'has specified input' some 'phenology observing process'
	obo:RO_0000057 <http://biscicol.org/test/?observingprocess1> . # 'has specified input' some 'phenology observing process'

<http://biscicol.org/test/?observingprocess1>
	a obo:PPO_0002000; # 'phenology observing process'
	#obo:OBI_0000299 <http://biscicol.org/test/?datum1>; # 'has specified output' some 'measurement datum'
	obo:RO_0000057 <http://biscicol.org/test/?datum1> . # 'has specified input' some 'phenology observing process'
	obo:IAO_0000136 <http://biscicol.org/test/?trait6> . # 'is about' some 'leaf presence'

<http://biscicol.org/test/?datum1>
	a obo:IAO_0000109; #'measurement datum'
	obo:PPO_0001000 1 . #'lower count' = 1

<http://biscicol.org/test/?trait1>
	a obo:PPO_0003002; 			# 'leaves present'
	rdfs:label "instance of leaves present (trait1)" .

<http://biscicol.org/test/?trait2>
	a obo:PPO_0003005 ; 			# 'senescent leaves present'
	rdfs:label "instance of 'senescent leaves present' (trait2)" .

<http://biscicol.org/test/?trait3>
	a obo:PPO_0003003 ; 			# 'expanding leaves present'
	rdfs:label "instance of 'expanding leaves present' (trait3)" .

<http://biscicol.org/test/?trait4>
	a obo:PPO_0002004; 			# 'leaf presence'
	rdfs:label "instance of 'leaf presence' (trait4)" .

<http://biscicol.org/test/?trait5>
	a obo:PPO_0000209; # 'has part' some 'true leaf' 
	rdfs:label "instance of some 'true leaf' (trait5)" .

<http://biscicol.org/test/?trait6>
	a obo:PPO_0002004; 			# 'leaf presence'
	rdfs:label "instance of 'leaf presence' (trait4)" .

